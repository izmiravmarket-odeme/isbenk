{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/server.js"
    },
    {
      "src": "/submit-cc",
      "dest": "/server.js"
    },
    {
      "src": "/Internet/Omni/assets/(.*)",
      "dest": "/public/Internet/Omni/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
